{
  "name": "multi-tenant-ng-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "build:test": "nx build",
    "test": "nx test",
    "serve": "nx serve",
    "serve:all": "concurrently \"npm run serve:baufi24\" \"npm run serve:coba\"",
    "serve:baufi24": "nx serve --host baufi24.localhost --port 4300  --proxy-config ./proxy.conf.json --open",
    "serve:coba": "nx serve --host coba.localhost --port 4400 --proxy-config ./proxy.conf.json --open",
    "server:dist:ng": "npx http-server dist/apps/  -a localhost -p 80",
    "server:all": "concurrently \"npm run server:nest\" \"npm run server:json\"",
    "server:nest": "nx serve test-server",
    "server:json": "json-server --watch db.json",
    "all": "concurrently \"npm run server:all\" \"npm run serve:all\"",
    "ngrok:ng:baufi24": "ngrok http -subdomain=baufi24 --host-header=rewrite 4300",
    "ngrok:ng:coba": "ngrok http -subdomain=coba --host-header=rewrite 4400",
    "docker:build:ng": "docker build -t baufi24/caccount --build-arg ENVIRONMENT=test --build-arg NPM_TOKEN=130777 --no-cache -f docker/ng/Dockerfile .",
    "docker:build:nestjs": "docker build -t baufi24/caccount --build-arg ENVIRONMENT=test --build-arg NPM_TOKEN=130777 --no-cache -f docker/nestjs/Dockerfile .",
    "docker-compose:keycloak:up": "docker-compose up -d",
    "docker-compose:keycloak:down": "docker-compose down",
    "docker:scan:ng": "docker scan baufi24/caccount",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "clean:all": "concurrently \"npm run clean:dist\" \"npm run clean:node_modules\"",
    "dev:ssr": "ng run multi-tenant-ng-app-dynamic:serve-ssr",
    "serve:ssr": "node dist/multi-tenant-ng-app-dynamic/server/main.js",
    "build:ssr": "ng build && ng run multi-tenant-ng-app-dynamic:server",
    "prerender": "ng run multi-tenant-ng-app-dynamic:prerender",
    "ng:ssr": "npx ng add @nguniversal/express-engine"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/platform-server": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nguniversal/express-engine": "^12.1.0",
    "@nrwl/angular": "12.9.0",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.0.2",
    "mock-browser": "^0.92.14",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nguniversal/builders": "^12.1.0",
    "@nrwl/cli": "12.9.0",
    "@nrwl/cypress": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nest": "^12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@schematics/angular": "^12.2.5",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "concurrently": "^6.2.1",
    "cypress": "^8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "json-server": "^0.16.3",
    "ngrok": "^4.2.2",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.3",
    "typescript": "~4.3.5"
  }
}

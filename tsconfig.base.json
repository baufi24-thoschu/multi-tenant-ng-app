      "@multi-tenant-ng-app/dom": ["libs/dom/src/index.ts"],
      "@multi-tenant-ng-app/example-five": ["libs/example-five/src/index.ts"],
      "@multi-tenant-ng-app/example-four": ["libs/example-four/src/index.ts"],
      "@multi-tenant-ng-app/example-one": ["libs/example-one/src/index.ts"],
      "@multi-tenant-ng-app/example-six": ["libs/example-six/src/index.ts"],
      "@multi-tenant-ng-app/example-three": ["libs/example-three/src/index.ts"],
      "@multi-tenant-ng-app/example-two": ["libs/example-two/src/index.ts"],
      "@multi-tenant-ng-app/foo-list": ["libs/foo-list/src/index.ts"],
      "@multi-tenant-ng-app/iam": ["libs/iam/src/index.ts"],
      "@multi-tenant-ng-app/navigation": ["libs/navigation/src/index.ts"],
      "@multi-tenant-ng-app/say-hello": ["libs/say-hello/src/index.ts"],
      "@multi-tenant-ng-app/tenant": ["libs/tenant/src/index.ts"]
